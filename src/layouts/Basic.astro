---
import BaseHead from "@/components/global/BaseHead.astro";
import Header from "@/components/global/Header.astro";
import Footer from "@/components/global/Footer.astro";
import Card from "@/components/Card.astro";
import Column from "@/components/Column.astro";
import Hero from "@/components/Hero.astro";
import Overscroll from "@/components/global/Overscroll.astro";
const { title, description, image } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead title={title} description={description} image={image} />
  </head>
  <body>
    <Header />
    <slot />
    <Footer />
    <Overscroll />
  </body><script src="../scripts/smoothTransition.js"></script>
</html>
